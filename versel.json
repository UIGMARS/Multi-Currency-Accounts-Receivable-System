{
  "builds": [
    {
      "src": "Multi-Currency-Accounts-Receivable-System/base/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxlambdasize": "45mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Multi-Currency-Accounts-Receivable-System/base/wsgi.py"
    }
  ]
}
